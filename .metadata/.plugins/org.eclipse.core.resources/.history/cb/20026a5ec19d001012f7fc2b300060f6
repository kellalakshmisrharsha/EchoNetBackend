package com.app;

import org.springframework.beans.factory.annotation.*;

import jakarta.persistence.*;

@Entity
public class User {
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	public Long user_id;
	
	@Column(name = "name")
	public String name;
	
	@Column(name="email")
	public String email;
	
	
	
}
